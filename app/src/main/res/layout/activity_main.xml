<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtMaNV"/>
    <Button
        android:id="@+id/btnhanvien"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nhân Viên"/>
    <Button
        android:id="@+id/btnSanPham"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sản Phẩm"/>

    <Button
        android:id="@+id/btnloaisanpham"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Loại Sản Phẩm"/>

    <Button
        android:id="@+id/btnNCC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nhà Cung Cấp" />
    <Button
        android:id="@+id/btnKhuVuc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Khu Vực" />
    <Button
        android:id="@+id/btnBan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bàn" />
    <Button
        android:id="@+id/btnHD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hóa đơn" />
    <Button
        android:id="@+id/btnDOIMK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Đỗi mật khẩu" />

</LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>